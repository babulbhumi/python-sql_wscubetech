   "source": [
    "query=\"\"\" select (sum(case when payment_installments >=1 then 1 else 0 end))/count(*)*100 from payments \"\"\"\n",
    "\n",
    "cur.execute(query)\n",
    "data= cur.fetchall()\n",
    "data"
